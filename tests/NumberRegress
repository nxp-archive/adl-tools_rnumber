: # -*- perl -*-
eval 'exec perl -w -S $0 ${1+"$@"}'
    if 0;

#
# Simple regression test for RValue iteration
#

$test = "./NumberTest";
$prog = "/bin/dc";

# Run the program
if (system("$test -dc $prog")) {
  print "Error executing test program $test\n";
  exit 1;
}

print "Number test PASSED.\n";

