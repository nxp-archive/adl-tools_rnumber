#ifndef __RutilFeature_h__
#define __RutilFeature_h__ "$Id$"

#ifdef __cplusplus
extern "C" {
#endif /* __cplusplus */


/* compile time version
 */
#define RNUMBER_MAJOR_VERSION				(@RNUMBER_MAJOR_VERSION@)
#define RNUMBER_MINOR_VERSION				(@RNUMBER_MINOR_VERSION@)
#define RNUMBER_MICRO_VERSION				(@RNUMBER_MICRO_VERSION@)
#define RNUMBER_BINARY_AGE				(@RNUMBER_BINARY_AGE@)
#define RNUMBER_INTERFACE_AGE				(@RNUMBER_INTERFACE_AGE@)
#define	RNUMBER_CHECK_VERSION(major,minor,micro)	\
    (RNUMBER_MAJOR_VERSION > (major) || \
     (RNUMBER_MAJOR_VERSION == (major) && RNUMBER_MINOR_VERSION > (minor)) || \
     (RNUMBER_MAJOR_VERSION == (major) && RNUMBER_MINOR_VERSION == (minor) && \
      RNUMBER_MICRO_VERSION >= (micro)))



#ifdef __cplusplus
}
#endif /* __cplusplus */


#endif
